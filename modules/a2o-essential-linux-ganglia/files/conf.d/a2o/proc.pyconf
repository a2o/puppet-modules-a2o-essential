modules {
    module {
	name     = "a2o_proc"
	language = "python"
    }
}



collection_group {
    collect_every  = 10
    time_threshold = 60
    metric {
	name            = "proc_forkrate"
	title           = "Process creation rate"
	value_threshold = 0.1
    }
    metric {
	name            = "sys_ctxt"
	title           = "Context switches"
	value_threshold = 0.1
    }
    metric {
	name            = "sys_intr"
	title           = "Interrupts"
	value_threshold = 0.1
    }
}
